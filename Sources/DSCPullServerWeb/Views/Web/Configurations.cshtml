<div class="row">
    <div class="col-md-12">
        <h1 class="page-header">@ViewBag.Title</h1>
    </div>
</div>

<div class="row">
    <div class="col-md-12" id="configurations-table">
        <div class="table-responsive">
            <table class="table table-hover">
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Checksum</th>
                        <th>Checksum Status</th>
                        <th></th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </div>
    </div>
    <div class="col-md-12" id="configurations-exception">
        <p class="text-danger bg-danger">500 (Internal Server Error)</p>
    </div>
    <div class="col-md-12" id="configurations-loader">
        <p>
            <i class="fa fa-spinner fa-spin fa-3x fa-fw"></i>

            <i class="fa fa-circle-o-notch fa-spin fa-3x fa-fw"></i>

            <i class="fa fa-refresh fa-spin fa-3x fa-fw"></i>

            <i class="fa fa-cog fa-spin fa-3x fa-fw"></i>

            <i class="fa fa-spinner fa-pulse fa-3x fa-fw"></i>
        </p>
    </div>
</div>
